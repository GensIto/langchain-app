{
	"name": "langchain-app",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"serve": "vite preview",
		"test": "vitest run",
		"deploy": "npm run build && wrangler deploy",
		"preview": "npm run build && vite preview",
		"cf-typegen": "wrangler types",
		"db:generate": "drizzle-kit generate",
		"db:push": "drizzle-kit push",
		"db:studio:local": "drizzle-kit studio --config drizzle.config.local.ts",
		"db:studio:prod": "drizzle-kit studio",
		"db:migrate:local": "wrangler d1 migrations apply langchain-app --local",
		"db:migrate:remote": "wrangler d1 migrations apply langchain-app --remote",
		"lint": "eslint . --ext .ts,.tsx,.js,.jsx",
		"lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
		"format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"check": "npm run lint:fix && npm run format",
		"ci:check": "npm run lint && npm run format:check"
	},
	"dependencies": {
		"@cloudflare/vite-plugin": "^1.13.8",
		"@langchain/cloudflare": "^1.0.2",
		"@langchain/core": "^1.1.22",
		"@logtape/logtape": "^2.0.2",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-devtools": "^0.7.0",
		"@tanstack/react-form": "^1.28.0",
		"@tanstack/react-router": "^1.132.0",
		"@tanstack/react-router-devtools": "^1.132.0",
		"@tanstack/react-router-ssr-query": "^1.131.7",
		"@tanstack/react-start": "^1.132.0",
		"@tanstack/router-plugin": "^1.132.0",
		"better-auth": "^1.4.18",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.45.1",
		"lucide-react": "^0.544.0",
		"next-themes": "^0.4.6",
		"radix-ui": "^1.4.3",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-markdown": "^10.1.0",
		"remark-gfm": "^4.0.1",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18",
		"vite-tsconfig-paths": "^5.1.4",
		"z": "^1.0.9"
	},
	"devDependencies": {
		"@tanstack/devtools-vite": "^0.3.11",
		"@tanstack/eslint-plugin-query": "^5.91.4",
		"@tanstack/eslint-plugin-router": "^1.155.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/react": "^16.2.0",
		"@types/node": "^22.19.11",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@vitejs/plugin-react": "^5.0.4",
		"drizzle-kit": "^0.31.9",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.5.5",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"jsdom": "^27.0.0",
		"prettier": "^3.8.1",
		"shadcn": "^3.8.4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.7.2",
		"vite": "^7.1.7",
		"vitest": "^3.0.5",
		"web-vitals": "^5.1.0",
		"wrangler": "^4.64.0"
	}
}
